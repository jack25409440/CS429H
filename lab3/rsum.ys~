#Author: Xiaohui Chen
#eid:xc2388


.pos 0
init:
irmovl Stack,%ebp
irmovl Stack,%esp
call Main
halt

.align 4
ele1: .long 0x00a
      .long ele2
ele2: .long 0x0b0
      .long ele3
ele3: .long 0xc00
      .long 0

Main:
pushl %ebp
rrmovl %esp,%ebp
irmovl ele1,%ecx
pushl %ecx
call rsum
rrmovl %esp,%ebp
popl %ebp
ret

rsum:
pushl %ebp
rrmovl %esp,%ebp
pushl %ebx
irmovl $4,%ecx
subl %ecx,%esp
irmovl $0,%eax
mrmovl 8(%ebp),%edx
addl %eax,%edx
je End
mrmovl (%edx),%ebx       #val
irmovl $4,%ecx
addl %ecx,%edx
mrmovl (%edx),%edx
rmmovl %edx,(%esp)
call rsum
addl %ebx,%eax


End:
irmovl $4,%ecx
addl %ecx,%esp          #rstore stack pointer
popl %ebx
popl %ebp
ret

.pos 0x100
Stack:
